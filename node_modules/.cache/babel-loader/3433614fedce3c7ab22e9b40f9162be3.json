etail>\r\n     </van-popup>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport { mapMutations, mapState } from 'vuex'\r\nimport PopDetail from './popDetail.vue'\r\nexport default {\r\n    computed: {\r\n        ...mapState([\"playList\", \"playListIndex\", \"isBtnShow\", \"detailShow\"])\r\n    },\r\n    methods: {\r\n        play: function () {\r\n            if (this.$refs.audio.paused) {\r\n                this.$refs.audio.play();\r\n                this.updataIsBtnShow(false);\r\n            }\r\n            else {\r\n                this.$refs.audio.pause();\r\n                this.updataIsBtnShow(true);\r\n            }\r\n        },\r\n        ...mapMutations([\"updataIsBtnShow\", \"updateDetailShow\"]),\r\n  },\r\n    updated() {\r\n      this.$store.dispatch(\"getLyric\",playList[playListIndex].id)\r\n    },\r\n    watch: {\r\n        playListIndex: function () {\r\n            this.$refs.audio.autoplay = true;\r\n            if (this.$refs.audio.paused) {\r\n                this.updataIsBtnShow(false);\r\n            }\r\n        },\r\n        playList: function () {\r\n            if (this.isBtnShow) {\r\n                this.$refs.audio.autoplay = true;\r\n                this.updataIsBtnShow(false);\r\n            }\r\n        }\r\n    },\r\n    components: { PopDetail }\r\n}\r\n\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.footer{\r\n    position: fixed;\r\n    width: 100%;\r\n    height: 1.2rem;\r\n    background-color: #fff;\r\n    bottom:0;\r\n    border-top: 1px solid #999;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    .footer-left{\r\n      width: 70%;\r\n      height: 100%;\r\n      display: flex;\r\n     img{\r\n      height: 1.5rem;\r\n      width: 1.5rem;\r\n    }\r\n    .musictext{\r\n      p{font-size: 0.1rem;}\r\n    }\r\n    }\r\n    .footer-right{\r\n      width: 30%;\r\n      height: 100%;\r\n      display: flex;\r\n      align-items: center;\r\n      .van-icon{\r\n        p